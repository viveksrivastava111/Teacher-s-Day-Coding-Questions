#include <iostream>
#include <unordered_map>
#include <string>
using namespace std;

int main() {
    int testCases;
    cin >> testCases;
    unordered_map<string, int> dataBase;
    string input;

    while (testCases--) {
        cin >> input;
        if (dataBase.count(input)) {
            int count = ++dataBase[input];
            cout << input << count << endl;
        } else {
            dataBase[input] = 0;
            cout << "OK" << endl;
        }
    }

    return 0;
}
